{
  "name": "princemuel",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "serve": "netlify dev",
    "build": "astro check && astro build",
    "build:prod": "astro check && astro build",
    "build:force": "astro check && astro build -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit -p tsconfig.json && biome lint --diagnostic-level=error --no-errors-on-unmatched .",
    "lint:fix": "biome check --write --unsafe .",
    "lint:staged": "biome check --error-on-warnings --no-errors-on-unmatched --staged .",
    "db:start": "supabase start",
    "db:status": "supabase status",
    "db:stop": "supabase stop",
    "db:link": "supabase link",
    "db:reset": "prisma db push",
    "db:introspect": "prisma db pull",
    "db:gen": "prisma generate",
    "db:sql": "prisma generate --sql --watch",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
